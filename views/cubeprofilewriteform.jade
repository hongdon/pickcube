extends layout

block content
	div(class='section')
		div(class='container')
			div(class='row')
				div(class='col-md-6')
					img(src='/images/Qiyi/146341915701abc6acb0244972ba36063c224870da__mn314401__w885__h590__f167542__Ym201605.jpg' class="img-responsive")
				
			div(class='row')
				div(class='col-md-12')
					form(role='form' action='/profilewrite' onsubmit='return check(this)')
						div(class='form-group')
						p 브랜드
							input(class="form-control" id="exampleInputEmail1" placeholder="brand" type="text")
						div(class='form-group')
							p 이름
							input(class="form-control" id="exampleInputEmail1" placeholder="name" type="text")
						div(class='form-group')
							p 코컷/역코컷
							input(class="form-control" id="exampleInputEmail1" placeholder="코컷/역코컷" type="text")
						div(class='form-group')
							p 회전감
							select(id='feel')
								option(value="") 회전감
								option(value="3") 상
								option(value="2") 중
								option(value="1") 하
						
						div(class='form-group')
							p 가격
							select(id='feel')
								option(value="") 가격
								option(value="3") 10000원이하
								option(value="2") 10000원대
								option(value="1") 20000원이상
							
						div(class='form-group')
							p 가성비
							select(id='feel')
								option(value="") 가성비
								option(value="5") 매우 좋음
								option(value="4") 좋음
								option(value="3") 보통
								option(value="2") 나쁨
								option(value="1") 매우 나쁨
							
						div(class='form-group')
							p comment
							input(class="form-control" id="exampleInputEmail1" placeholder="comment" type="text")
						button(type="submit" class="btn btn-default") Submit
	script(scr='/resourcesProfile/check.js', charset='utf-8')
																			