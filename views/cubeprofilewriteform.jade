extends layout

block content
	script(type='text/javascript' src='/resourcesProfile/check.js', charset='utf-8')

	div(class='section')
		div(class='container')
			div(class='row')
				div(class='col-md-6' id='field')
					//img(id='imageaddr' src='/images/Qiyi/146341915701abc6acb0244972ba36063c224870da__mn314401__w885__h590__f167542__Ym201605.jpg' class="img-responsive")
					
					form(role='form' method="post" action="/file-upload" name ='imgform' id='imgform' enctype="multipart/form-data" )
						p 이미지
						input(input type="file" name="thumbnail")
						input(input type="submit")
			div(class='row')
				div(class='col-md-12')
					form(role='form' action='/profilewrite' method='POST' onsubmit='return check(this)')
						
						
						
								
						div(class='form-group')
						p 브랜드
							input(class="form-control" id="brand" name = 'brand' placeholder="brand" type="text")
						div(class='form-group')
							p 이름
							input(class="form-control" id="name" name='name' placeholder="name" type="text")
						div(class='form-group')
							p 코컷
							input(class="form-control" id="cc" name='cc' placeholder="코컷" type="text")
						div(class='form-group')
							p 역코컷
							input(class="form-control" id="ccc" name='ccc' placeholder="역코컷" type="text")
						div(class='form-group')
							p 회전감
							input(class="form-control" id="feel" name='feel' placeholder="회전감" type="text")
						div(class='form-group')
							p 가격
							input(class="form-control" id="price" name='price' placeholder="가격" type="text")
						div(class='form-group')
							p 가성비
							input(class="form-control" id="goodorbad" name='goodorbad' placeholder="가성비" type="text")
						
						div(class='form-group')
							p comment
							input(class="form-control" id="comment" name='comment' placeholder="comment" type="text")
						button(type="submit" class="btn btn-default") Submit
																			